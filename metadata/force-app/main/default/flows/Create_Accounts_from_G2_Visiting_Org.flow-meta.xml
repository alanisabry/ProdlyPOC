        <name>CountryMapping</name>
        <dataType>String</dataType>
        <expression>CASE({!$Record.G2Crowd__Country__c}, 
&quot;Czechia&quot;, &quot;Czech Republic&quot;,
&quot;Russia&quot; , &quot;Russian Federation&quot;,
&quot;Vietnam&quot;, &quot;Viet Nam&quot;,
{!$Record.G2Crowd__Country__c} )</expression>
    </formulas>
    <formulas>
        <name>G2EventCountryMapping</name>
        <dataType>String</dataType>
        <expression>CASE({!Get_G2_Event.G2Crowd__Country__c}, 
&quot;Czechia&quot;, &quot;Czech Republic&quot;,
&quot;Russia&quot; , &quot;Russian Federation&quot;,
{!Get_G2_Event.G2Crowd__Country__c})</expression>
