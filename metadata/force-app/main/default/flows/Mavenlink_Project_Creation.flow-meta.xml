    <decisions>
        <name>Currency_on_Account_and_Opportunity_the_same</name>
        <label>Currency on Account and Opportunity the same?</label>
        <locationX>920</locationX>
        <locationY>237</locationY>
        <defaultConnector>
            <targetReference>Update_Account_currency</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Yes_currency_the_same</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>recordAccount.CurrencyIsoCode</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>recordOpportunity.CurrencyIsoCode</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assign_Mavenlink_Project_fields</targetReference>
            </connector>
            <label>Yes, currency the same</label>
        </rules>
    </decisions>
    <decisions>
        <description>Checks whether the project has already been created or not.</description>
        <name>Project_can_be_created</name>
        <label>Project can be created?</label>
        <locationX>591</locationX>
        <locationY>235</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Yes</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>recordOpportunity.ML_project_created__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>recordOpportunity.StageName</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Closed Won</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Fetch_Account_information</targetReference>
            </connector>
            <label>Yes</label>
        </rules>
    </decisions>
    <description>Flow that creates a Mavenlink project. Triggered by clicking a link on the opportunity.</description>
    <environments>Default</environments>
    <formulas>
        <name>Budget</name>
        <dataType>String</dataType>
        <expression>TEXT(ROUND({!recordOpportunity.One_off_from_Products__c}, 2))</expression>
    </formulas>
    <formulas>
        <description>Value for the Mavenlink project.</description>
        <name>MRRvalue</name>
        <dataType>String</dataType>
        <expression>CASE({!recordOpportunity.CurrencyIsoCode},
&quot;USD&quot;, &quot;$&quot;,
&quot;EUR&quot;, &quot;€&quot;,
&quot;GBP&quot;, &quot;£&quot;,
&quot;AED&quot;, &quot;AED&quot;,
TEXT({!recordOpportunity.CurrencyIsoCode})) &amp; TEXT(ROUND({!recordOpportunity.MRR_from_Products__c},0))</expression>
    </formulas>
    <interviewLabel>Mavenlink Project Creation {!$Flow.CurrentDateTime}</interviewLabel>
    <label>Mavenlink Project Creation</label>
    <processMetadataValues>
