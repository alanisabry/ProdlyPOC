    <environments>Default</environments>
    <formulas>
        <name>FirstLevelPrice</name>
        <dataType>Currency</dataType>
        <expression>IF(
	LEFT({!RecordId},3)={!$Label.OLIIdPrefix},
	({!Get_OLI.Quantity}*{!First_Evaluation_Rule.Unit_List_Price__c})/{!Get_OLI.Quantity},
	IF(
		LEFT({!RecordId}, 3)={!$Label.QLIIdPrefix},
		({!Get_QLI.Quantity}*{!First_Evaluation_Rule.Unit_List_Price__c})/{!Get_QLI.Quantity},
		0
	)
)</expression>
        <scale>8</scale>
    </formulas>
    <formulas>
        <name>FirstLevelQLIUnitPrice</name>
        <dataType>Currency</dataType>
        <expression>ROUND({!FirstLevelPrice}/ 12, 8)</expression>
        <scale>17</scale>
    </formulas>
    <formulas>
        <name>MRRValuaFormula</name>
        <dataType>Currency</dataType>
        <expression>IF(
	LEFT({!RecordId},3)={!$Label.OLIIdPrefix},
	ROUND({!UpdatedOLI.Sales_Price_ARR__c}/ 12, 8),
	IF(
		LEFT({!RecordId}, 3)={!$Label.QLIIdPrefix},
		ROUND({!UpdatedQLI.Sales_Price_ARR_One_off__c}/ 12, 8),
		0
	)
)</expression>
        <scale>17</scale>
    </formulas>
    <formulas>
        <name>SecondLevelPrice</name>
        <dataType>Currency</dataType>
        <expression>IF(
	LEFT({!RecordId},3)={!$Label.OLIIdPrefix},
	((
	{!First_Evaluation_Rule.Unit_List_Price__c} * {!First_Evaluation_Rule.Max_Qty__c} 
	)+ ({!Second_Evaluation_Rule.Unit_List_Price__c} * ({!Get_OLI.Quantity} - {!First_Evaluation_Rule.Max_Qty__c})))/{!Get_OLI.Quantity},
	IF(
		LEFT({!RecordId}, 3)={!$Label.QLIIdPrefix},
		((
	{!First_Evaluation_Rule.Unit_List_Price__c} * {!First_Evaluation_Rule.Max_Qty__c} 
	)+ ({!Second_Evaluation_Rule.Unit_List_Price__c} * ({!Get_QLI.Quantity} - {!First_Evaluation_Rule.Max_Qty__c})))/{!Get_QLI.Quantity},
		0
	)
)</expression>
        <scale>8</scale>
    </formulas>
    <formulas>
        <name>SecondLevelQLIUnitPrice</name>
        <dataType>Currency</dataType>
        <expression>ROUND({!SecondLevelPrice}/ 12, 8)</expression>
        <scale>17</scale>
    </formulas>
    <formulas>
        <name>ThirdLevelPrice</name>
        <dataType>Currency</dataType>
        <expression>IF(
	LEFT({!RecordId},3)={!$Label.OLIIdPrefix},
	(({!First_Evaluation_Rule.Unit_List_Price__c} * {!First_Evaluation_Rule.Max_Qty__c}) + 
({!Second_Evaluation_Rule.Unit_List_Price__c} * ({!Second_Evaluation_Rule.Max_Qty__c} - {!First_Evaluation_Rule.Max_Qty__c})) + 
({!Third_Evaluation_Rule.Unit_List_Price__c} * ({!Get_OLI.Quantity} - {!Second_Evaluation_Rule.Max_Qty__c}))
)/{!Get_OLI.Quantity},
	IF(
		LEFT({!RecordId}, 3)={!$Label.QLIIdPrefix},
		(({!First_Evaluation_Rule.Unit_List_Price__c} * {!First_Evaluation_Rule.Max_Qty__c}) + 
({!Second_Evaluation_Rule.Unit_List_Price__c} * ({!Second_Evaluation_Rule.Max_Qty__c} - {!First_Evaluation_Rule.Max_Qty__c})) + 
({!Third_Evaluation_Rule.Unit_List_Price__c} * ({!Get_QLI.Quantity} - {!Second_Evaluation_Rule.Max_Qty__c}))
)/{!Get_QLI.Quantity},
		0
	)
)</expression>
        <scale>8</scale>
    </formulas>
    <formulas>
        <name>ThirdLevelQLIUnitPrice</name>
        <dataType>Currency</dataType>
        <expression>ROUND({!ThirdLevelPrice}/ 12, 8)</expression>
        <scale>17</scale>
    </formulas>
