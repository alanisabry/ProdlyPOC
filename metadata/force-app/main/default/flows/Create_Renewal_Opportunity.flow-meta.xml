    <description>BODEV-4533 Tracing New Pricing Deals
Updated the flow to copy &quot;Opportunity with value-based pricing&quot; field to the Renewal Opportunity</description>
    <environments>Default</environments>
    <formulas>
        <name>defaultPricePackageNonPackage</name>
        <dataType>Currency</dataType>
        <expression>IF({!VarT_PartnerName} = &apos;Workfront&apos; &amp;&amp;  NOT(ISBLANK({!Sov_OLI.PricebookEntry.Workfront_Renewal_Price__c})) , {!Sov_OLI.PricebookEntry.Workfront_Renewal_Price__c},
IF((TEXT({!Sov_OLI.Recurring_Discount__c}) = &quot;Yes&quot; || {!Sov_OLI.UnitPrice} &gt; {!Sov_OLI.Default_Price__c}), {!Sov_OLI.UnitPrice},  
{!Sov_OLI.Default_Price__c}))</expression>
        <scale>8</scale>
    </formulas>
    <formulas>
        <name>InitialContractARR</name>
        <dataType>Currency</dataType>
        <expression>IF(OR(
ISPICKVAL({!VarT_OpportunityType}, &quot;New Business-Enterprise Cloud&quot;),
ISPICKVAL({!VarT_OpportunityType}, &quot;New Business - Expansion Portal&quot;),
ISPICKVAL({!VarT_OpportunityType}, &quot;Existing Business - Internal Transfer&quot;),
ISPICKVAL({!VarT_OpportunityType}, &quot;Existing Business - Platform Transfer&quot;)
),{!Sov_OLI.Sales_Price_ARR__c},{!Sov_OLI.Initial_Purchased_Price_ARR__c})</expression>
        <scale>8</scale>
    </formulas>
    <formulas>
        <name>NewContractStartDate</name>
        <dataType>Date</dataType>
        <expression>{!VarD_ContractEndDate} +1</expression>
    </formulas>
    <formulas>
        <name>NewEndDate</name>
        <dataType>Date</dataType>
        <expression>DATE( Year({!NewContractStartDate})+floor((MONTH({!NewContractStartDate}) +{!VarN_RenewalTerm} - 1) / 12) , 
    mod(MONTH({!NewContractStartDate}) + {!VarN_RenewalTerm} -1, 12) + 1 , 
     day({!NewContractStartDate})
) - 1</expression>
    </formulas>
    <formulas>
        <description>Returns new OLI Sales Price in ARR/One-off</description>
        <name>NewOLIARRPrice</name>
        <dataType>Currency</dataType>
        <expression>/*
Renewal price will be populated in renewal Opportunities, 
only applicable for Webdam Workfront Opportuniteis
*/
IF(
	AND({!VarT_PartnerName} &lt;&gt; &quot;Workfront&quot;, ISPICKVAL ({!Sov_OLI.Recurring_Discount__c}, &quot;Yes&quot;)), {!Sov_OLI.Sales_Price_ARR__c},
  		IF(
    		{!VarT_PartnerName} = &quot;Workfront&quot;, 
    		{!WorkfrontRenewalPrice} * 12,  
               IF({!Sov_OLI.UnitPrice} &gt; {!Sov_OLI.Default_Price__c}, {!Sov_OLI.Sales_Price_ARR__c},
               {!Sov_OLI.Default_Price_ARR__c}
      )
  )
)</expression>
        <scale>8</scale>
    </formulas>
    <formulas>
        <name>NewOLIPrice</name>
        <dataType>Currency</dataType>
        <expression>/*
Renewal price will be populated in renewal Opportunities, 
only applicable for Webdam Workfront Opportuniteis
*/
IF(
	AND({!VarT_PartnerName} &lt;&gt; &quot;Workfront&quot;, ISPICKVAL ({!Sov_OLI.Recurring_Discount__c}, &quot;Yes&quot;)), {!Sov_OLI.UnitPrice},
  		IF(
    		{!VarT_PartnerName} = &quot;Workfront&quot;, 
    		{!WorkfrontRenewalPrice},  
               IF({!Sov_OLI.UnitPrice} &gt; {!Sov_OLI.Default_Price__c}, {!Sov_OLI.UnitPrice},
               {!Sov_OLI.Default_Price__c}
      )
  )
)</expression>
        <scale>8</scale>
    </formulas>
    <formulas>
        <name>Nowand1sec</name>
        <dataType>DateTime</dataType>
        <expression>NOW()+0.000011</expression>
    </formulas>
    <formulas>
        <name>OLIDiscountAmount</name>
        <dataType>Currency</dataType>
        <expression>IF (ISPICKVAL({!Sov_OLI.Recurring_Discount__c}, &quot;No&quot;), 0,{!Sov_OLI.Discount_Amount__c} )</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>OLIRecurringDiscount</name>
        <dataType>String</dataType>
        <expression>IF ({!defaultPricePackageNonPackage} = {!NewOLIPrice}, &quot;&quot;, IF(ISPICKVAL ({!Sov_OLI.Recurring_Discount__c}, &quot;Yes&quot;), &quot;Yes&quot;, &quot;&quot;))</expression>
    </formulas>
    <formulas>
        <name>TODAYPLUS180DAYS</name>
        <dataType>Date</dataType>
        <expression>{!$Flow.CurrentDate} + 180</expression>
    </formulas>
    <formulas>
        <description>Returns Renewal price for Workfront products.</description>
        <name>WorkfrontRenewalPrice</name>
        <dataType>Currency</dataType>
        <expression>  IF(AND(NOT(ISBLANK({!Sov_OLI.PricebookEntry.Workfront_Renewal_Price__c})), ISPICKVAL({!Sov_OLI.Recurring_Discount__c}, &quot;Yes&quot;)),
  	{!Sov_OLI.PricebookEntry.Workfront_Renewal_Price__c} * (1 - {!Sov_OLI.Discount_percentage__c}),
  	IF(NOT(ISBLANK({!Sov_OLI.PricebookEntry.Workfront_Renewal_Price__c})),
  	{!Sov_OLI.PricebookEntry.Workfront_Renewal_Price__c},
  	IF(ISPICKVAL({!Sov_OLI.Recurring_Discount__c}, &quot;Yes&quot;),
  	{!Sov_OLI.UnitPrice},
        IF({!Sov_OLI.UnitPrice} &gt; {!Sov_OLI.Default_Price__c}, {!Sov_OLI.UnitPrice},
  	{!Sov_OLI.Default_Price__c}))))</expression>
        <scale>8</scale>
    </formulas>
