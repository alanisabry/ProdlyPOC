        <name>LastProRatedLimit</name>
        <dataType>Number</dataType>
        <expression>ROUND(
(({!Loop_through_Yearly_Contracted_Limits.annual_limit_value}*{!Loop_through_Yearly_Contracted_Limits.qty})/365)*(({!Loop_through_Yearly_Contracted_Limits.opp_end_date}- {!varLastStartDate})+1),3)</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>LastTerm</name>
        <dataType>Number</dataType>
        <expression>{!varTermNumber} - 1</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>MonthlyProratedLimitValue</name>
        <dataType>Number</dataType>
        <expression>ROUND(
(({!Loop_through_Yearly_Contracted_Limits.annual_limit_value}*{!Loop_through_Yearly_Contracted_Limits.qty})/365)*(({!varLastDayofMonth} - {!varFirstDayofMonth})+1),3)</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>varFirstDayofNextTerm</name>
        <dataType>Date</dataType>
        <expression>{!varLastDayofTerm} + 1</expression>
    </formulas>
    <formulas>
        <name>varLastDayofMonth</name>
        <dataType>Date</dataType>
        <expression>DATE(
  YEAR({!varFirstDayofMonth}) + FLOOR((MONTH({!varFirstDayofMonth}) + 1) / 13),
  MOD(MONTH({!varFirstDayofMonth}) + 1 - 1, 12) + 1,
  1
) - 1</expression>
    </formulas>
    <formulas>
        <name>varLastDayofTerm</name>
        <dataType>Date</dataType>
        <expression>DATE(YEAR({!varFirstDayofTerm}) + 1, MONTH({!varFirstDayofTerm}), DAY({!varFirstDayofTerm})) - 1</expression>
    </formulas>
    <formulas>
        <name>varLastMonthOfTermProRatedLimit</name>
        <dataType>Number</dataType>
        <expression>ROUND(({!Loop_through_Yearly_Contracted_Limits.annual_limit_value}/365)*(({!varLastDayofTerm}- {!varFirstDayofMonth})+1),3)</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>varLastStartDate</name>
        <dataType>Date</dataType>
        <expression>DATE(YEAR({!Loop_through_Yearly_Contracted_Limits.opp_end_date}), MONTH({!Loop_through_Yearly_Contracted_Limits.opp_end_date}), 1)</expression>
    </formulas>
    <formulas>
        <name>varNextFirstDayofMonth</name>
        <dataType>Date</dataType>
        <expression>{!varLastDayofMonth} + 1</expression>
    </formulas>
    <formulas>
        <name>varNextFirstDayofTerm</name>
        <dataType>Date</dataType>
        <expression>DATE(YEAR({!Loop_through_Yearly_Contracted_Limits.opp_start_date}) + {!varTermNumber} -1, MONTH({!Loop_through_Yearly_Contracted_Limits.opp_start_date}), DAY({!Loop_through_Yearly_Contracted_Limits.opp_start_date}))</expression>
    </formulas>
    <formulas>
        <name>varNumberofTerms</name>
        <dataType>Number</dataType>
        <expression>{!Loop_through_Yearly_Contracted_Limits.contract_term}/12</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>varSameMonth</name>
        <dataType>Boolean</dataType>
        <expression>(MONTH({!varLastDayofMonth}) = MONTH({!varLastDayofTerm})) 
&amp;&amp; (YEAR({!varLastDayofMonth}) = YEAR({!varLastDayofTerm}))</expression>
    </formulas>
