        <name>Currency</name>
        <dataType>String</dataType>
        <expression>CASE(TEXT({!Current_User.Work_Location__c}), 
&quot;Amsterdam&quot;, &quot;EUR&quot;,
&quot;London&quot;, &quot;GBP&quot;,
&quot;Dubai&quot;, &quot;AED&quot;,
&quot;San Mateo&quot;, &quot;USD&quot;,
&quot;San Carlos&quot;, &quot;USD&quot;,
&quot;AUD&quot;)</expression>
    </formulas>
