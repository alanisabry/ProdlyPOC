        <name>Name</name>
        <dataType>String</dataType>
        <expression>IF(ISBLANK(TEXT({!Account.BillingCountryCode})), &quot;[country abbreviation] - &quot;, TEXT({!Account.BillingCountryCode}) + &quot; - &quot;) + {!Account.Name} + &quot; - Contract 1 - Transfer&quot;</expression>
    </formulas>
