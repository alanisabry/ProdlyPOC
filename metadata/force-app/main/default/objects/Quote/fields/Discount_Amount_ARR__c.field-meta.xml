<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Discount_Amount_ARR__c</fullName>
    <description>Indicates the discount given on the ARR amount in the quote.</description>
    <formula>ROUND(ARR_Before_Discount__c - ARR__c,2)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Discount Amount - ARR</label>
    <precision>18</precision>
    <required>false</required>
    <scale>8</scale>
    <trackHistory>false</trackHistory>
    <type>Currency</type>
</CustomField>
