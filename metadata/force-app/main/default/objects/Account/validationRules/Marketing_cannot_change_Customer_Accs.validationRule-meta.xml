<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Marketing_cannot_change_Customer_Accs</fullName>
    <active>false</active>
    <errorConditionFormula>AND( 
OR(RecordType.Name = &apos;Customer&apos;, RecordType.Name = &apos;Webdam&apos;), 
CONTAINS($Profile.Name,&quot;Marketing&quot;) 
)</errorConditionFormula>
    <errorMessage>Marketing users cannot change information of customer accounts.</errorMessage>
</ValidationRule>
