<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Match_Split_One_off_Subscr_Invoice__c</fullName>
    <description>Indicates whether the submitted value matches the current value on Account/Opportunity.</description>
    <externalId>false</externalId>
    <formula>IF(
  ISBLANK( Form_Submitted_Timestamp__c ), &quot;&quot;,
  IF(
    Split_One_off_Subscription_Invoice__c = Opportunity__r.Split_one_off_subscription_invoice__c,
    IMAGE(&quot;/servlet/servlet.FileDownload?file=0150P000005PRdt&quot;,&quot;YES&quot;),
    IMAGE(&quot;/servlet/servlet.FileDownload?file=0150P000005PRcW&quot;, &quot;NO&quot;) + &quot; &quot; + IF(Opportunity__r.Split_one_off_subscription_invoice__c, &quot;Yes&quot;, &quot;No&quot;)
  )
)</formula>
    <label>Match? - Split One-off/Subscr. Invoice</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
