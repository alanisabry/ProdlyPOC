<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Conga_Discount_percentage_One_Off__c</fullName>
    <description>BODEV-2180 Update the table format in Order Form</description>
    <externalId>false</externalId>
    <formula>IF( Conga_Package_Default_price_One_Off__c &gt; Conga_Package_Total_price_One_off__c , ROUND((((Conga_Package_Total_price_One_off__c - Conga_Package_Default_price_One_Off__c)/Conga_Package_Default_price_One_Off__c)*100),0),0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Conga Discount percentage One - Off</label>
    <precision>18</precision>
