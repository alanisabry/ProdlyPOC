<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Total_Contract_Value__c</fullName>
    <description>One-off + (MRR*Contract Term).  If Contract Term is blank then assumed at 12 months.</description>
    <formula>IF(OR(ISBLANK(Subscription_Term__c),Subscription_Term__c=0),One_off__c+(MRR_from_Products__c*12),One_off__c+(MRR_from_Products__c*
IF(TEXT(Special_Billing_Scenario__c) = &quot;Free Month Discount&quot;,Subscription_Term__c - of_Month_Delayed_Rent_Free__c,Subscription_Term__c)))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Total Contract Value</label>
    <precision>18</precision>
    <required>false</required>
    <scale>4</scale>
    <trackHistory>false</trackHistory>
    <type>Currency</type>
</CustomField>
