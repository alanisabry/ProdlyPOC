<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ARR__c</fullName>
    <description>Annual Recurring Revenue</description>
    <formula>IF(AND(
ISPICKVAL(StageName,&quot;Closed Won&quot;),
HasOpportunityLineItem = TRUE), MRR_from_Products__c * 12, MRR__c *12)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Annual Recurring Revenue</inlineHelpText>
    <label>ARR from products</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <type>Currency</type>
</CustomField>
