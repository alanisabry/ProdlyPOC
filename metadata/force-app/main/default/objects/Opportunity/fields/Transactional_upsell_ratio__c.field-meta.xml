<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Transactional_upsell_ratio__c</fullName>
    <externalId>false</externalId>
    <formula>( of_products_Storage__c + of_products_Users__c) / (of_opportunity_products__c - of_products_One_Off_Items__c)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>1.0 means the upsells consists fully out of storage &amp; user products
Less than 1 means it&apos;s non-transactional</inlineHelpText>
    <label>Transactional upsell ratio</label>
    <precision>18</precision>
    <required>false</required>
    <scale>1</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
