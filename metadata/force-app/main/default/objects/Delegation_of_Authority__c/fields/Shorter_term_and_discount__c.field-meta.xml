<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Shorter_term_and_discount__c</fullName>
    <formula>AND(ISPICKVAL(Opportunity__r.Type,&apos;New Business-Enterprise Cloud&apos;),Opportunity__r.Netsuite_Term__c &lt; 36,Discount_ARR__c &gt;0, NOT(ISPICKVAL(Opportunity__r.Account.Company_Size_New__c,&apos;SMB&apos;)))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>This field is checked if the contract term is &lt; 3 years and ARR discount is added. Not applicable for SMB.</inlineHelpText>
    <label>Shorter term and discount? doa 25</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
