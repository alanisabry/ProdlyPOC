<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Netsuite_Quantity__c</fullName>
    <description>Inverts the sign of the quantity for credit memos.</description>
    <externalId>false</externalId>
    <formula>IF(Order.Total_Order_Value__c&lt;0, -1*Quantity,Quantity)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Inverts the sign of the quantity for credit memos.</inlineHelpText>
    <label>Netsuite Quantity</label>
    <precision>18</precision>
    <required>false</required>
    <scale>4</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
