<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Total_Subscription_Fee__c</fullName>
    <description>Created this field to calculate Sub-Total with Price Increase</description>
    <externalId>false</externalId>
    <formula>Subscription_Fee__c +  MAX(Price_Increase__c,0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Total Subscription Fee</label>
    <precision>18</precision>
    <required>false</required>
    <scale>8</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
