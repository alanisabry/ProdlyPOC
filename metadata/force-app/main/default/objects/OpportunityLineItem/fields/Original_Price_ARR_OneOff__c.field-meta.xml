<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Original_Price_ARR_OneOff__c</fullName>
    <description>Returns ARR price of this products from the previous contract.</description>
    <formula>IF(
 For_Renewal__c = True,
 Original_Price__c * 12,
 Original_Price__c)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Previous Contract ARR/One-off</label>
    <precision>18</precision>
    <required>false</required>
    <scale>8</scale>
    <type>Currency</type>
</CustomField>
