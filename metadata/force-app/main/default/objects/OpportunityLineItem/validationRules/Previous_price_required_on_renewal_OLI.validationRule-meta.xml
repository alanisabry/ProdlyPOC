<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Previous_price_required_on_renewal_OLI</fullName>
    <active>false</active>
    <errorConditionFormula>AND(TEXT(Opportunity.Type) = &apos;Existing Business-Renewal&apos;,
     For_Renewal__c,
    ISBLANK(Original_Price__c)
    )</errorConditionFormula>
    <errorMessage>Previous contract price is required on renewal products</errorMessage>
</ValidationRule>
