<?xml version="1.0" encoding="UTF-8"?>
<ListView xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>My_Salesforce_Connection_Leads</fullName>
    <columns>FULL_NAME</columns>
    <columns>LEAD.COMPANY</columns>
    <columns>LEAD.EMAIL</columns>
    <columns>LEAD.STATUS</columns>
    <columns>Lead_Free__c</columns>
    <filterScope>Mine</filterScope>
    <filters>
        <field>LEAD.TITLE</field>
        <operation>contains</operation>
        <value>Head,VP,President,SVP,Vice President,Manager,Innovation,Digital,Marketing</value>
    </filters>
    <filters>
        <field>Company_Size_New__c</field>
        <operation>equals</operation>
        <value>Mid-Market,Enterprise,Strategic</value>
    </filters>
    <filters>
        <field>LEAD.TITLE</field>
        <operation>notEqual</operation>
        <value>Account Exec,Assistant,AE,Account Executive,Lecturer,Student,Intern,Professor</value>
    </filters>
    <label>My Salesforce Connection Leads</label>
</ListView>
