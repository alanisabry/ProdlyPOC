<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Cumulative_Indexation_Amount__c</fullName>
    <description>If Cumulative Indexation is applied, Total Initial Default Price ARR/One-off * Cumulative Indexation %, otherwise 0.</description>
    <formula>IF( Pricing_Factors_to_be_Applied__c &amp;&amp; Quote__r.Applied_Cumulative_Indexation__c !=null,
Total_Initial_Default_Price_ARR_One_off__c * Quote__r.Applied_Cumulative_Indexation__c,
0
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Cumulative Indexation Amount</label>
    <precision>18</precision>
    <required>false</required>
    <scale>8</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Currency</type>
</CustomField>
