<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Order_End_Date</fullName>
    <active>true</active>
    <errorConditionFormula>AND(ISBLANK(EndDate),
ISPICKVAL(Status,&quot;Activated&quot;))</errorConditionFormula>
    <errorDisplayField>EndDate</errorDisplayField>
    <errorMessage>Please enter the order end date. For PS this can be the same at the start date.</errorMessage>
</ValidationRule>
