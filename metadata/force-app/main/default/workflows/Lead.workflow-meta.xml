    <rules>
        <fullName>Change Livechat lead source</fullName>
        <actions>
            <name>Change_Livechat_lead_source</name>
            <type>FieldUpdate</type>
        </actions>
        <active>false</active>
        <criteriaItems>
            <field>Lead.LeadSource</field>
            <operation>equals</operation>
            <value>LiveChat</value>
        </criteriaItems>
        <triggerType>onCreateOnly</triggerType>
    </rules>
    <rules>
        <fullName>Lead Owner Timestamp</fullName>
        <actions>
            <name>Owner_Timestamp_Change</name>
            <type>FieldUpdate</type>
        </actions>
        <active>false</active>
        <description>Sets a timestamp when lead owner is changed</description>
        <formula>AND(ISCHANGED(OwnerId),  $User.Id &lt;&gt; &quot;0050P000007hH92&quot;, Owner:User.Id&lt;&gt; &quot;005U0000002Yo5Y&quot;)</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>S5%3A Unresponsive %3E Nurture if no activity</fullName>
        <actions>
            <name>Sales_Nurture</name>
            <type>FieldUpdate</type>
        </actions>
        <active>false</active>
        <criteriaItems>
            <field>Lead.Days_since_last_activity__c</field>
            <operation>greaterOrEqual</operation>
            <value>30</value>
        </criteriaItems>
        <criteriaItems>
            <field>Lead.Status</field>
            <operation>equals</operation>
            <value>Sales 5: Unresponsive</value>
        </criteriaItems>
        <description>If a lead is in Sales 5: Unresponsive with no activity for 30 days, add to Sales Nurture</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Sets %22Interested%22 to true</fullName>
        <active>false</active>
        <criteriaItems>
            <field>Lead.Status</field>
            <operation>equals</operation>
            <value>Sales 4a: Interested</value>
        </criteriaItems>
        <description>Sets the field interested to true if lead is Sales 4a</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Timestamp Exit Date</fullName>
        <active>false</active>
        <criteriaItems>
            <field>Lead.Status</field>
            <operation>equals</operation>
            <value>Sales 3a: Not appropriate contact,Sales 4b: Not Interested,Sales 5: Unresponsive,Nurture,Unqualified</value>
        </criteriaItems>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Timestamp Marketing 1</fullName>
        <active>false</active>
        <criteriaItems>
            <field>Lead.Status</field>
            <operation>equals</operation>
            <value>Marketing 1: Not assessed</value>
        </criteriaItems>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Update LS 1 and LS 2 number fields</fullName>
        <active>false</active>
        <formula>OR (
IsNew (),
IsChanged (LastModifiedDate))</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
